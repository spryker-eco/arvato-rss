{
  "name": "spryker-eco/arvato-rss",
  "description": "Arvato risk security check module",
  "license": "MIT",
  "require": {
    "ext-soap": "*",
    "spryker/kernel": "^3.0.0",
    "spryker/money": "^2.1.0",
    "spryker/config": "^3.0.0",
    "spryker/transfer": "^3.4",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-0": {
      "SprykerEco\\": "src/",
      "Functional\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/spryker.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/architecture-sniffer.git"
    },
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    },
    {
      "type": "git",
      "url": "https://github.com/spryker-eco/arvato-rss.git"
    }
  ],
  "config": {
    "sort-packages": true
  }
}
